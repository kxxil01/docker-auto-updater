{
  "check_interval": 30,
  "services": [
    {
      "name": "service-name",
      "image": "image-registry-url",
      "compose_file": "compose-file-path",
      "compose_service": "compose-service-name",
      "registry_type": "ecr",
      "tag_pattern": "staging-*",
      "enabled": true,
      "registry_config": {
        "region": "region",
        "aws_access_key_id": "aws-access-key-id",
        "aws_secret_access_key": "aws-secret-access-key"
      }
    },
    {
      "name": "service-name",
      "image": "image-registry-url",
      "compose_file": "compose-file-path",
      "compose_service": "compose-service-name",
      "registry_type": "ecr",
      "tag_pattern": "v*",
      "enabled": true,
      "registry_config": {
        "region": "region",
        "aws_access_key_id": "aws-access-key-id",
        "aws_secret_access_key": "aws-secret-access-key"
      }
    }
  ]
}
